<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="davijo" id="fishtyp-01">
		<createTable tableName="fishingtype">
			<column name="fishtyp_id" type="BIGINT">
				<constraints primaryKey="true" primaryKeyName="fishtyp_pk" />
			</column>
			<column name="fishtyp_code" type="VARCHAR(2)" />
			<column name="fishtyp_desc" type="VARCHAR(60)" />
			<column name="fishtyp_updattim" type="TIMESTAMP WITHOUT TIME ZONE" />
			<column name="fishtyp_upuser" type="VARCHAR(60)" />
		</createTable>
		<rollback>
			<dropTable tableName="fishingtype" cascadeConstraints="true"/>
		</rollback>
	</changeSet>
    
    
</databaseChangeLog>
