<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="davijo" id="fishgm-01">
		<createTable tableName="fishinggearmobility">
			<column name="fishgm_id" type="BIGINT">
				<constraints primaryKey="true" primaryKeyName="fishgm_pk" />
			</column>
			<column name="fishgm_code" type="VARCHAR(1)" />
			<column name="fishgm_desc" type="VARCHAR(200)" />
			<column name="fishgm_updattim" type="TIMESTAMP WITHOUT TIME ZONE" />
			<column name="fishgm_upuser" type="VARCHAR(60)" />
		</createTable>
		<rollback>
			<dropTable tableName="fishinggearmobility" cascadeConstraints="true"/>
		</rollback>		
	</changeSet>
    
    
</databaseChangeLog>
