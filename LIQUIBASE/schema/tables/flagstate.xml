<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="johlyn" id="FLAGSTATE type table">
		<createTable tableName="flagstate">
			<column name="flagstate_id" type="BIGINT">
				<constraints primaryKey="true" primaryKeyName="flagstate_pk" />
			</column>
            <column name="flagstate_code" type="VARCHAR(3)" />
			<column name="flagstate_name" type="VARCHAR(25)" />
			<column name="flagstate_updattim" type="TIMESTAMP WITHOUT TIME ZONE" />
			<column name="flagstate_upuser" type="VARCHAR(60)" />
		</createTable>
		<createSequence sequenceName="flagstate_id_seq"/>
		<rollback>
			<dropTable tableName="flagstate" cascadeConstraints="true"/>
		</rollback>
	</changeSet>
	<changeSet author="nhily" id="Remove old sequence">
		<dropSequence sequenceName="flagstate_id_seq"/>
	</changeSet>
</databaseChangeLog>
