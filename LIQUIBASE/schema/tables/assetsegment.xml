<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="johlyn" id="ASSET.SEGMENT create table">
		<createTable tableName="assetsegment">
			<column name="seg_id" type="BIGINT">
				<constraints primaryKey="true" primaryKeyName="assetseg_pk" />
			</column>
			<column name="seg_code" type="VARCHAR(20)" />
			<column name="seg_desc" type="VARCHAR(40)" />
			<column name="seg_updattim" type="TIMESTAMP WITHOUT TIME ZONE" />
			<column name="seg_upuser" type="VARCHAR(60)" />
		</createTable>
		<rollback>
			<dropTable tableName="assetsegment" cascadeConstraints="true"/>
		</rollback>
	</changeSet>
    
</databaseChangeLog>
