<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="davijo" id="fishgtyp-01">
		<createTable tableName="fishinggeartype">
			<column name="fishgtyp_id" type="BIGINT">
				<constraints primaryKey="true" primaryKeyName="fishgtyp_pk" />
			</column>
			<column name="fishgtyp_name" type="VARCHAR(30)" />
			<column name="fishgtyp_desc" type="VARCHAR(60)" />
			<column name="fishgtyp_updattim" type="TIMESTAMP WITHOUT TIME ZONE" />
			<column name="fishgtyp_upuser" type="VARCHAR(60)" />
		</createTable>
		<rollback>
			<dropTable tableName="fishinggeartype" cascadeConstraints="true"/>
		</rollback>
	</changeSet>
	
	<changeSet author="nhily" id="fishingGearTypeCode">
		<addColumn tableName="fishinggeartype">
			<column name="fishgtyp_code" type="BIGINT"/>
		</addColumn>
		<rollback>
			<dropColumn columnName="fishgtyp_code" tableName="fishinggeartype"/>
		</rollback>
	</changeSet>
	
    
</databaseChangeLog>
