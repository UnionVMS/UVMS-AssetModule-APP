<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="davijo" id="hullm-01">
		<createTable tableName="hullmaterial">
			<column name="hullm_id" type="BIGINT">
				<constraints primaryKey="true" primaryKeyName="hullm_pk" />
			</column>
			<column name="hullm_material" type="VARCHAR(22)" />
			<column name="hullm_desc" type="VARCHAR(200)" />
			<column name="hullm_updattim" type="TIMESTAMP WITHOUT TIME ZONE" />
			<column name="hullm_upuser" type="VARCHAR(60)" />
		</createTable>
		<rollback>
			<dropTable tableName="hullmaterial" cascadeConstraints="true"/>
		</rollback>
	</changeSet>
    
</databaseChangeLog>
