<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="davijo" id="pubaid-03">
		<insert tableName="publicaid">
			<column name="pubaid_id" valueNumeric="1" />
			<column name="pubaid_code" value="AE" />
			<column name="pubaid_desc" value="Aid not defined by the union." />
			<column name="pubaid_updattim" valueDate="${now}" />
			<column name="pubaid_upuser" value="UVMS" />
		</insert>
		<insert tableName="publicaid">
			<column name="pubaid_id" valueNumeric="2" />
			<column name="pubaid_code" value="AC" />
			<column name="pubaid_desc" value="Aid defined by the union." />
			<column name="pubaid_updattim" valueDate="${now}" />
			<column name="pubaid_upuser" value="UVMS" />
		</insert>
		<insert tableName="publicaid">
			<column name="pubaid_id" valueNumeric="3" />
			<column name="pubaid_code" value="PA" />
			<column name="pubaid_desc" value="No aid." />
			<column name="pubaid_updattim" valueDate="${now}" />
			<column name="pubaid_upuser" value="UVMS" />
		</insert>
		<insert tableName="publicaid">
			<column name="pubaid_id" valueNumeric="4" />
			<column name="pubaid_code" value="EI" />
			<column name="pubaid_desc"
				value="Aid under the conditions that the engine power are reduced (individual)." />
			<column name="pubaid_updattim" valueDate="${now}" />
			<column name="pubaid_upuser" value="UVMS" />
		</insert>
		<insert tableName="publicaid">
			<column name="pubaid_id" valueNumeric="5" />
			<column name="pubaid_code" value="EG" />
			<column name="pubaid_desc"
				value="Aid under the conditions that the engine power are reduced (group)." />
			<column name="pubaid_updattim" valueDate="${now}" />
			<column name="pubaid_upuser" value="UVMS" />
		</insert>
		<rollback>
			<delete tableName="publicaid">
				<where>
					pubaid_id in (1,2,3,4,5)
				</where>
			</delete>
		</rollback>
	</changeSet>
</databaseChangeLog>
