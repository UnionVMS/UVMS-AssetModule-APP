<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="davijo" id="fishtyp-03">
		<insert tableName="fishingtype">
			<column name="fishtyp_id" valueNumeric="1" />
			<column name="fishtyp_code" value="P" />
			<column name="fishtyp_desc" value="Pelagic." />
			<column name="fishtyp_updattim" valueDate="${now}" />
			<column name="fishtyp_upuser" value="UVMS" />
		</insert>
		<insert tableName="fishingtype">
			<column name="fishtyp_id" valueNumeric="2" />
			<column name="fishtyp_code" value="D" />
			<column name="fishtyp_desc" value="Dermersal." />
			<column name="fishtyp_updattim" valueDate="${now}" />
			<column name="fishtyp_upuser" value="UVMS" />
		</insert>
		<insert tableName="fishingtype">
			<column name="fishtyp_id" valueNumeric="3" />
			<column name="fishtyp_code" value="DP" />
			<column name="fishtyp_desc" value="Demersal and Pelagic." />
			<column name="fishtyp_updattim" valueDate="${now}" />
			<column name="fishtyp_upuser" value="UVMS" />
		</insert>
		<insert tableName="fishingtype">
			<column name="fishtyp_id" valueNumeric="4" />
			<column name="fishtyp_code" value="U" />
			<column name="fishtyp_desc" value="Unknown." />
			<column name="fishtyp_updattim" valueDate="${now}" />
			<column name="fishtyp_upuser" value="UVMS" />
		</insert>
		<rollback>
			<delete tableName="fishingtype">
				<where>
					fishtyp_id in (1,2,3, 4)
				</where>
			</delete>
		</rollback>
	</changeSet>
</databaseChangeLog>
