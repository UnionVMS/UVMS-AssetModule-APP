<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

	<changeSet author="davijo" id="hullm-03">
		<insert tableName="hullmaterial">
			<column name="hullm_id" valueNumeric="1" />
			<column name="hullm_material" value="WOOD" />
			<column name="hullm_desc" value="Wooden hull." />
			<column name="hullm_updattim" valueDate="${now}" />
			<column name="hullm_upuser" value="UVMS" />
		</insert>
		<insert tableName="hullmaterial">
			<column name="hullm_id" valueNumeric="2" />
			<column name="hullm_material" value="METAL" />
			<column name="hullm_desc" value="Metal hull." />
			<column name="hullm_updattim" valueDate="${now}" />
			<column name="hullm_upuser" value="UVMS" />
		</insert>
		<insert tableName="hullmaterial">
			<column name="hullm_id" valueNumeric="3" />
			<column name="hullm_material" value="GLAS_PLASTIC_FIBER" />
			<column name="hullm_desc" value="Glas or plastic fiber hull." />
			<column name="hullm_updattim" valueDate="${now}" />
			<column name="hullm_upuser" value="UVMS" />
		</insert>
		<insert tableName="hullmaterial">
			<column name="hullm_id" valueNumeric="4" />
			<column name="hullm_material" value="OTHER" />
			<column name="hullm_desc" value="Other." />
			<column name="hullm_updattim" valueDate="${now}" />
			<column name="hullm_upuser" value="UVMS" />
		</insert>
		<insert tableName="hullmaterial">
			<column name="hullm_id" valueNumeric="5" />
			<column name="hullm_material" value="UNKNOWN" />
			<column name="hullm_desc" value="Unknown." />
			<column name="hullm_updattim" valueDate="${now}" />
			<column name="hullm_upuser" value="UVMS" />
		</insert>
		<rollback>
			<delete tableName="hullmaterial">
				<where>
					hullm_id in (1,2,3,4,5)
				</where>
			</delete>
		</rollback>
	</changeSet>
</databaseChangeLog>
